use std::str::FromStr;
use associated_types_lib::ParseCallbacks;

// FIXME: the 'a shouldn't be needed
grammar<'a, P>(callbacks: &'a mut P) where P: ParseCallbacks, P: 'a;

pub Term: P::Term = {
    <n:Num> => n.into(),
    "(" <t:Term> ")" => t,
};

Num: P::Num = <s:r"[0-9]+"> => callbacks.number(i32::from_str(s).unwrap());
